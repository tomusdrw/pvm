{
	"name": "anan-as",
	"version": "1.0.0",
	"main": "./bin/index.js",
	"scripts": {
		"asbuild": "npm run asbuild:debug && npm run asbuild:release",
		"asbuild:debug": "asc assembly/index.ts --target debug",
		"asbuild:release": "asc assembly/index.ts --target release",
		"cp-build": "rm -rf ./web/build; cp -r ./build ./web/",
		"build": "npm run asbuild && npm run cp-build",
		"format": "biome format --write",
		"lint": "biome lint --write; biome check --write",
		"qa": "biome ci",
		"qa-fix": "npm run format; npm run lint",
		"prestart": "npm run build",
		"preweb": "npm run build",
		"web": "npx live-server ./web",
		"start": "node ./bin/index.js"
	},
	"keywords": [],
	"author": "Fluffy Labs",
	"license": "MPL-2.0",
	"description": "",
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@jazzer.js/core": "^2.1.0",
		"@typeberry/pvm-debugger-adapter": "^0.0.1-09fe4ca",
		"assemblyscript": "^0.27.31"
	},
	"type": "module",
	"exports": {
		".": {
			"import": "./build/release.js",
			"types": "./build/release.d.ts"
		}
	},
	"dependencies": {
		"json-bigint-patch": "^0.0.8"
	}
}
